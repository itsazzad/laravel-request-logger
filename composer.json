{
  "name": "prettus/laravel-request-logger",
  "description": "HTTP request logger middleware for Laravel",
  "keywords": ["laravel", "logger", "request", "response"],
  "license": "MIT",
  "authors": [
    {
      "name": "Anderson Andrade",
      "email": "contato@andersonandra.de",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "contato@andersonandra.de",
    "issues":"https://github.com/andersao/laravel-request-logger/issues",
    "wiki":"https://github.com/andersao/laravel-request-logger",
    "source":"https://github.com/andersao/laravel-request-logger"
  },
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "5.0.*|~5.1|~5.2",
    "illuminate/http": "5.0.*|~5.1|~5.2",
    "monolog/monolog": "~1.11"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\RequestLogger\\": "src/Prettus/RequestLogger/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.1-dev"
    }
  },
  "minimum-stability": "stable"
}
